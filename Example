Tab:AddButton({ 
    Name = "Fe Hat Universe",
    Callback = function()
      local LC = game.Players.LocalPlayer local Name = LC.Name local Char = LC.Character local Humanoid = Char:FindFirstChildWhichIsA("Humanoid") local Root = Humanoid.RootPart local Accessories = Humanoid:GetAccessories() local Target = Char local TargetPos = Target.Humanoid.RootPart.Position local Offset = 10 local Rotation = 0 local Speed = 1 local Height = 2 local Power = 50000 local Damping = 800 local Mode = 1 local Following = true local Spin = true local Rotating = true local AkaliNotif = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/Dynissimo/main/Scripts/AkaliNotif.lua"))(); local Notify = AkaliNotif.Notify; Notify({ 	Description = "This script was fully made by DD26"; 	Title = "FE Hat Universe"; 	Duration = 10; }); Notify({ 	Description = "If you want to showcase this script, please use the rekonise link."; 	Title = "FE Hat Universe"; 	Duration = 20; }); function findName(pname) 	for i, v in ipairs(game.Players:GetPlayers()) do 		if pname then 			if string.match(v.Name:lower(), pname:lower()) or string.match(v.Character.Humanoid.DisplayName:lower(), pname:lower()) then 				return v.Name 			end 		else 		end 	end end function findChar(pname) 	return game.Players:FindFirstChild(findName(pname)).Character end function Netless() 	for i,v in next, game:GetService("Players").LocalPlayer.Character:GetDescendants() do 		if v:IsA("BasePart") and v.Name ~="HumanoidRootPart" then 			game:GetService("RunService").Heartbeat:connect(function() 				v.Velocity = Vector3.new(-30,0,0) 			end) 		end 	end end Netless() local hats = {} if Target then 	local character = Target 	for _, hat in ipairs(character:GetChildren()) do 		if hat:IsA("Accessory") then 			hats[#hats+1] = hat 		end 	end end local hatCount = #hats if hatCount > 0 then 	local angle = math.pi * 2 / hatCount 	for i, hat in ipairs(hats) do 		local handle = hat.Handle 		handle.AccessoryWeld:Remove() 		if handle then 			local bodyPosition = Instance.new("BodyPosition", handle) 			bodyPosition.MaxForce = Vector3.new(500000, 500000, 500000) 			bodyPosition.P = Power 			bodyPosition.D = Damping 			local bodyGyro = Instance.new("BodyGyro", handle) 			bodyGyro.MaxTorque = Vector3.new(500000, 500000, 500000) 			bodyGyro.P = Power 			bodyGyro.D = Damping 			local x = math.sin(Rotation + angle * (i-1))*Offset 			local z = math.cos(Rotation + angle * (i-1))*Offset 			bodyPosition.Position = TargetPos + Vector3.new(x, Height, z) 		end 	end 	local function myCoroutine() 		while wait(-9e999) do 			Rotation = Rotation + (Speed / 20) 			if Rotation >= math.pi * 2 then 				Rotation = 0 			end 			for i, hat in ipairs(hats) do 				local handle = hat.Handle 				local x = math.sin(Rotation + angle * (i-1))*Offset 				local z = math.cos(Rotation + angle * (i-1))*Offset 				handle.BodyPosition.P = Power 				handle.Velocity = Vector3.new(0, 0, 0) 				handle.Massless = true 				handle.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0) 				if Rotating == true then 					if Accessories[i+1] then 						handle.BodyGyro.CFrame = CFrame.lookAt(handle.Position, Accessories[i+1].Handle.Position) 					elseif not Accessories[i+1] then 						handle.BodyGyro.CFrame = CFrame.lookAt(handle.Position, Accessories[i-1].Handle.Position) 					else 					end 				end 				if Spin == true then 					handle.BodyPosition.Position = TargetPos + Vector3.new(x + Target.Humanoid.MoveDirection.X, Height, z + Target.Humanoid.MoveDirection.Z) 				end 				if Following == true then 					if Target == Char then 						TargetPos = Target.Humanoid.RootPart.Position+(Target.Humanoid.MoveDirection+Target.Humanoid.RootPart.Velocity)/150 					else 						TargetPos = Target.Humanoid.RootPart.Position+(Target.Humanoid.MoveDirection+Target.Humanoid.RootPart.Velocity)/5 					end 				end 			end 		end 	end 	local myWrappedCoroutine = coroutine.wrap(myCoroutine) 	myWrappedCoroutine() end function Mode2() 	if Mode == 2 then 		local Angle = math.pi * 2 / #hats 		function Loop() 			if Mode == 2 then 				local RootOrientation = Target.Humanoid.RootPart.CFrame - Target.Humanoid.RootPart.Position 				local RootRotation = RootOrientation 				for i, Hat in ipairs(hats) do 					local HatRotation = RootRotation.Y + Angle * (i - 1) + Speed * tick() 					local x = math.sin(HatRotation)*Offset 					local z = math.cos(HatRotation)*Offset 					local HatPos = TargetPos + RootOrientation * Vector3.new(x, z, -Height) 					Hat:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = HatPos 				end 				wait() 				Loop() 			end 		end 		Loop() 		for i, Hat in ipairs(hats) do 			local Handle = Hat.Handle 			Hat:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos 		end 	end end function Mode3() 	if Mode == 3 then 		for i = 1, #Accessories do 			Accessories[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(0, Height, 0) 			wait(.15) 		end 		wait(.15) 		Mode3() 	end end function Mode4 () 	if Mode == 4 then 		if not LC:GetMouse().Target then else 			TargetPos = LC:GetMouse().Hit.Position 		end 		wait(-9e999) 		Mode4() 	end end function Mode5 () 	local spiralPitch = 0 	local spiralAngle = 0 	function Loop () 		if Mode == 5 then 			spiralAngle = spiralAngle + Speed / 300 			if spiralAngle >= math.pi * 10 then 				spiralAngle = 0 			end 			for i, hat in ipairs(hats) do 				local handle = hat.Handle 				if handle then 					local x = math.sin(spiralAngle + i * spiralPitch) * (i*Offset / 8) 					local y = i * (Height / 3) 					local z = math.cos(spiralAngle + i * spiralPitch) * (i*Offset / 8) 					handle.BodyPosition.Position = TargetPos - Vector3.new(0, 2, 0) + Vector3.new(x, y, z) 				end 			end 		end 		spiralPitch += Speed / 70 		wait(-9e999) 		Loop() 	end 	Loop() end function Mode6 () 	local stack1 = {} 	local stack2 = {} 	for i = 1, #Accessories do 		if i <= #Accessories / 2 then 			stack1[#stack1 + 1] = Accessories[i] 		else 			stack2[#stack2 + 1] = Accessories[i] 		end 	end 	function Loop() 		if Mode == 6 then 			local angle = tick() * Speed 			local x = math.sin(angle)*Offset 			local z = math.cos(angle)*Offset 			for i, v in ipairs(stack1) do 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, i+Height,-z) 			end 			for i, v in ipairs(stack2) do 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(-x, i+Height,z) 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode7() 	local stack1 = {} 	local stack2 = {} 	local stack3 = {} 	for i = 1, #Accessories do 		if i < #Accessories / 3 then 			stack1[#stack1 + 1] = Accessories[i] 		elseif i < #Accessories / 3 * 2 or i == #Accessories then 			stack2[#stack2 + 1] = Accessories[i] 		else 			stack3[#stack3 + 1] = Accessories[i] 		end 	end 	function Loop() 		if Mode == 7 then 			local angle = tick() * Speed 			local x = math.sin(angle)*Offset 			local z = math.cos(angle)*Offset 			for i, v in ipairs(stack1) do 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, i+Height, -z) 			end 			for i, v in ipairs(stack2) do 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, i+Height, z) 			end 			for i, v in ipairs(stack3) do 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(-x, i+Height, -z) 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode8() 	if Mode == 8 then 		local forward = workspace.CurrentCamera.CFrame.LookVector 		local right = workspace.CurrentCamera.CFrame.RightVector 		local up = workspace.CurrentCamera.CFrame.UpVector 		local angle = math.pi * 2 / #hats * tick() 		for i, hat in ipairs(hats) do 			local handle = hat.Handle 			local x = right * (math.sin(angle * (i-1))*Offset) 			local y = up * (math.cos(angle * (i-1))*Offset) 			local z = forward * (Height+10) 			local pos = workspace.CurrentCamera.CFrame.LookVector + z + x + y 			local look = (workspace.CurrentCamera.CFrame.LookVector - pos).unit 			handle.BodyPosition.Position = pos + TargetPos + Vector3.new(0, 2, 0) 		end 		wait() 		Mode8() 	end end function Mode9 () 	local Circle1 = {} 	local Circle2 = {} 	for i, v in pairs(Accessories) do 		if (#Circle1 < #Accessories / 2) then 			Circle1[#Circle1 + 1] = v 		else 			Circle2[#Circle2 + 1] = v 		end 	end 	function Loop () 		if Mode == 9 then 			for i = 1, #Circle1 do 				local angle = tick() * Speed 				local x = math.sin(angle + (i * 5))*Offset 				local z = math.cos(angle + (i * 5))*Offset 				Circle1[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, Height, z) 			end 			for i = 1, #Circle2 do 				local angle = tick() * Speed 				local x = math.sin(angle + (i * 5))*Offset 				local z = math.cos(angle + (i * 5))*Offset 				Circle2[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(z, Height, x) 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode10 () 	local Left = {} 	local Right = {} 	for i, v in pairs(Accessories) do 		if (#Left < #Accessories / 2) then 			Left[#Left + 1] = v 		else 			Right[#Right + 1] = v 		end 	end 	function Loop () 		if Mode == 10 then 			for i, v in ipairs(Left) do 				local angle = tick() * Speed 				local x = math.sin(angle + i)*Offset 				local z = math.cos(angle + i)*Offset 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(z, x + Height, -x) 			end 			for i, v in ipairs(Right) do 				local angle = tick() * Speed 				local x = math.sin(angle + i)*Offset 				local z = math.cos(angle + i)*Offset 				v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(-x, x + Height, -z) 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode11 () 	local OldOffset = Offset 	local Circle1 = {} 	local Circle2 = {} 	for i, v in pairs(Accessories) do 		if (#Circle1 < #Accessories / 2) then 			Circle1[#Circle1 + 1] = v 		else 			Circle2[#Circle2 + 1] = v 		end 	end 	function Loop () 		if Mode == 11 then 			for i = 1, #Circle1 do 				local angle = tick() * Speed 				local x = -math.sin(angle + (i * angle))*Offset 				local y = math.cos(angle) / 2 * OldOffset 				local z = math.cos(angle + (i * -angle))*Offset 				Offset = math.sin(angle) / 2 * OldOffset 				local offset = CFrame.Angles(0,math.rad( Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(x, Height+y, z) 				Circle1[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			for i = 1, #Circle2 do 				local angle = tick() * Speed 				local x = -math.sin(angle + (i * angle))*Offset 				local y = -math.cos(angle) / 2 * OldOffset 				local z = math.cos(angle + (i * angle))*Offset 				Offset = math.sin(angle) / 2 * OldOffset 				local offset = CFrame.Angles(0, math.rad(Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(x, Height+y, z) 				Circle2[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode12 () 	local Circle1 = {} 	local Circle2 = {} 	for i, v in pairs(Accessories) do 		if (#Circle1 < #Accessories / 2) then 			Circle1[#Circle1 + 1] = v 		else 			Circle2[#Circle2 + 1] = v 		end 	end 	function Loop () 		if Mode == 12 then 			for i = 1, #Circle1 do 				local angle = tick() * Speed 				local x = math.sin(angle + (i * 5))*Offset 				local z = math.cos(angle + (i * 5))*Offset 				local offset = CFrame.Angles(0, math.rad(Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(x, Height, z) 				Circle1[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			for i = 1, #Circle2 do 				local angle = tick() * Speed 				local x = math.sin(angle + (i * 5))*Offset 				local z = math.cos(angle + (i * 5))*Offset 				local offset = CFrame.Angles(0, math.rad(-Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(x, Height + 2, z) 				Circle2[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode13 () 	local Circle1 = {} 	local Circle2 = {} 	for i, v in pairs(Accessories) do 		if (#Circle1 < #Accessories / 2) then 			Circle1[#Circle1 + 1] = v 		else 			Circle2[#Circle2 + 1] = v 		end 	end 	function Loop () 		if Mode == 13 then 			for i = 1, #Circle1 do 				local angle = tick() * Speed 				local x = math.sin(angle + (i * 5))*Offset 				local z = math.cos(angle + (i * 5))*Offset 				local offset = CFrame.Angles(0, math.rad(Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(x + Offset * 2, Height, z) 				Circle1[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			for i = 1, #Circle2 do 				local angle = tick() * Speed 				local x = math.sin(angle + (i * 5))*Offset 				local z = math.cos(angle + (i * 5))*Offset 				local offset = CFrame.Angles(0, math.rad(Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(x - Offset * 2, Height, z) 				Circle2[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode14() 	if Mode == 14 then 		for i, v in pairs(Accessories) do 			local Angle = math.sin(tick())*Speed 			local x = math.sin(Angle * (i-1)) * Offset 			local y = math.sin(Angle * (i-1)) + ((i*Height/5)+Angle) 			local z = math.cos(Angle * (i-1)) * Offset 			 			v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, y-2, z) 		end 		wait() 		Mode14() 	end end function Mode15() 	Height = -1 	function Loop () 		if Mode == 15 then 			for i = 1, #Accessories do 				local offset = CFrame.Angles(0, math.rad(Target.Humanoid.RootPart.Orientation.Y), 0) * Vector3.new(0, Height, -i*Offset / 5) 				Accessories[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + offset 			end 			wait() 			Loop() 		end 	end 	Loop() end function Mode16() 	local function Loop() 		if Mode == 16 then 			for i, v in pairs(Accessories) do 				local x = math.cos(math.random(1, 255) + (i + 1))*Offset 				local z = math.sin(math.random(1, 255) + (i + 1))*Offset 				local m = math.random(1, 13) 				if m == 1 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, Height, z) 				elseif m == 2 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(z, Height, x) 				elseif m == 3 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(-x, Height, z) 				elseif m == 4 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, Height, -z) 				elseif m == 5 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, z, z) 				elseif m == 6 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, x, z) 				elseif m == 7 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(-x, x, z) 				elseif m == 8 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, -x, z) 				elseif m == 9 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, x, -z) 				elseif m == 10 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(-x, z, z)	 				elseif m == 11 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, -z, z)	 				elseif m == 12 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, z, -z)	 				elseif m == 13 then 					v:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(z, z, z)		 				end 			end 		end 		wait() 		Loop() 	end 	Loop() end function Mode17() 	local Circle1 = {} 	local Circle2 = {} 	local Offset2 = 0 	local Height2 = 0 	for i, v in pairs(Accessories) do 		if (#Circle1 < #Accessories / 2) then 			Circle1[#Circle1 + 1] = v 		else 			Circle2[#Circle2 + 1] = v 		end 	end 	function Loop () 		if Mode == 17 then 			local angle = tick() * Speed+1 			for i = 1, #Circle1 do 				local x = math.cos(angle + (i * #hats))*Offset2/2 				local z = math.sin(angle + (i * #hats))*Offset2/2 				Circle1[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, -Height2, z) 			end 			for i = 1, #Circle2 do 				local x = math.cos(angle + (i * #hats))*Offset2/2 				local z = math.sin(angle + (i * #hats))*Offset2/2 				Circle2[i]:FindFirstChild("Handle"):FindFirstChild("BodyPosition").Position = TargetPos + Vector3.new(x, Height2, z) 			end 			Offset2 = math.sin(angle) * Offset 			Height2 = math.cos(angle) * Height 			wait() 			Loop() 		end 	end 	Loop() end local connect = LC.Chatted:Connect(function(chat) 	local Split = chat:lower():split(" ") 	local C1 = Split[1] 	local C2 = Split[2] 	if C1 == ".mode" then 		Mode = tonumber(C2) 		if C2 == "1" then 			Following = true 			Spin = true 			Rotating = true 		elseif C2 == "2" then 			Following = true 			Spin = false 			Rotating = true 			Mode2()		 		elseif C2 == "3" then 			Following = true 			Spin = false 			Rotating = true 			Mode3() 		elseif C2 == "4" then 			Following = false 			Spin = true 			Rotating = true 			Mode4() 		elseif C2 == "5" then 			Following = true 			Spin = false 			Rotating = true 			Mode5() 		elseif C2 == "6" then 			Following = true 			Spin = false 			Rotating = true 			Mode6() 		elseif C2 == "7" then 			Following = true 			Spin = false 			Rotating = true 			Mode7() 		elseif C2 == "8" then 			Following = true 			Spin = false 			Rotating = true 			Mode8() 		elseif C2 == "9" then 			Following = true 			Spin = false 			Rotating = true 			Mode9() 		elseif C2 == "10" then 			Following = true 			S